{
  "name": "react-simple-country-select",
  "version": "0.0.0-semantically-released",
  "description": "React component for country select",
  "main": "dist/index.js",
  "engines": {
    "node": "> 4",
    "npm": "> 3"
  },
  "scripts": {
    "add-contributor": "kcd-scripts contributors add",
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "validate": "kcd-scripts validate",
    "setup": "npm install && npm run validate -s",
    "precommit": "kcd-scripts precommit",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "predeploy": "styleguidist build",
    "deploy": "gh-pages -d styleguide",
    "generate-countries-json": "node other/generate-countries-json.js"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "stereobooster <stereobooster@gmail.com>",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=0.14.x"
  },
  "devDependencies": {
    "babel-loader": "7.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "gh-pages": "1.2.0",
    "kcd-scripts": "0.40",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-styleguidist": "7.3.4",
    "webpack": "4.17.1",
    "world-countries": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "import/extensions": "off",
      "react/display-name": "off",
      "react/prop-types": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "styleguide",
    "other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/stereobooster/react-simple-country-select.git"
  },
  "bugs": {
    "url": "https://github.com/stereobooster/react-simple-country-select/issues"
  },
  "homepage": "https://github.com/stereobooster/react-simple-country-select#readme"
}
